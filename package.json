{
  "name": "xs-components",
  "version": "0.0.1",
  "repository": {
    "type": "git",
    "url": "git://github.com:jsxiaosi/xiaosiCommitLib.git"
  },
  "workspaces": [
    "packages/*",
    "docs"
  ],
  "author": "",
  "license": "ISC",
  "scripts": {
    "ct": "bash ./script/template.sh",
    "clean": "rimraf ./lib",
    "dev": "cd effect && npm run dev",
    "test": "echo \"Error: no test specified\" && exit 1",
    "register:components": "vitepress-rc",
    "log": "conventional-changelog -p cmyr-config -i CHANGELOG.md -s -r 0",
    "type:web": "vue-tsc -p tsconfig.web.json --composite false --noEmit",
    "type:effect": "vue-tsc -p tsconfig.effect.json --composite false --noEmit",
    "type:node": "tsc -p tsconfig.node.json --composite false --noEmit",
    "docs:dev": "vitepress dev docs",
    "docs:build": "vitepress build docs",
    "docs:serve": "vitepress serve docs --port 9581",
    "build": "gulp --require @esbuild-kit/cjs-loader -f build/gulpfile.ts",
    "build:theme": "gulp --require @esbuild-kit/cjs-loader -f build/gulpfile.prod.ts",
    "lint:eslint": "eslint \"{packages,docs}/**/*.{vue,ts,js}\" --fix",
    "lint:prettier": "prettier --write --loglevel warn \"src/**/*.{js,json,css,scss,vue,html,md}\"",
    "lint:stylelint": "stylelint --fix \"**/*.{vue,less,postcss,css,scss}\" --cache --cache-location node_modules/.cache/stylelint/",
    "lint:pretty": "pretty-quick --staged",
    "lint:staged": "lint-staged",
    "commit": "git-cz",
    "prepare": "husky install"
  },
  "devDependencies": {
    "@algolia/client-search": "^4.14.2",
    "@commitlint/cli": "^17.1.2",
    "@commitlint/config-conventional": "^17.1.0",
    "@esbuild-kit/cjs-loader": "^2.3.3",
    "@rollup/plugin-commonjs": "^22.0.2",
    "@rollup/plugin-node-resolve": "^13.3.0",
    "@types/fs-extra": "^9.0.13",
    "@types/gulp": "^4.0.9",
    "@types/gulp-autoprefixer": "^0.0.33",
    "@types/gulp-clean-css": "^4.3.0",
    "@types/gulp-postcss": "^8.0.2",
    "@types/gulp-sass": "^5.0.0",
    "@types/markdown-it-container": "^2.0.5",
    "@types/node": "^18.7.15",
    "@types/prismjs": "^1.26.0",
    "@typescript-eslint/eslint-plugin": "^5.36.1",
    "@typescript-eslint/parser": "^5.36.1",
    "@vitejs/plugin-vue": "^3.1.0",
    "@vitejs/plugin-vue-jsx": "^2.0.1",
    "@vue/compiler-sfc": "^3.2.38",
    "@vue/tsconfig": "^0.1.3",
    "chalk": "5.0.1",
    "consola": "^2.15.3",
    "conventional-changelog-cli": "^2.2.2",
    "cz-conventional-changelog": "^3.3.0",
    "cz-customizable": "^7.0.0",
    "del": "^7.0.0",
    "esbuild": "^0.15.7",
    "eslint": "^8.23.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-promise": "^6.0.1",
    "eslint-plugin-vue": "^9.4.0",
    "fast-glob": "^3.2.11",
    "gulp": "^4.0.2",
    "gulp-autoprefixer": "^8.0.0",
    "gulp-clean-css": "^4.3.0",
    "gulp-postcss": "^9.0.1",
    "gulp-sass": "^5.1.0",
    "husky": "^8.0.1",
    "kolorist": "^1.5.1",
    "lint-staged": "^13.0.3",
    "postcss": "^8.4.16",
    "postcss-html": "^1.5.0",
    "postcss-import": "^15.0.0",
    "postcss-pxtorem": "^6.0.0",
    "prettier": "2.7.1",
    "pretty-quick": "^3.1.3",
    "rollup": "^2.79.0",
    "rollup-plugin-esbuild": "^4.10.1",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-typescript2": "^0.33.0",
    "rollup-plugin-vue": "^6.0.0",
    "sass": "^1.54.8",
    "stylelint": "^14.11.0",
    "stylelint-config-prettier": "^9.0.3",
    "stylelint-config-recess-order": "^3.0.0",
    "stylelint-config-standard": "^28.0.0",
    "stylelint-order": "^5.0.0",
    "stylelint-scss": "^4.3.0",
    "ts-morph": "^16.0.0",
    "typescript": "^4.8.2",
    "unplugin-vue-macros": "^0.11.1",
    "vite": "^3.1.0",
    "vue": "^3.2.38",
    "vue-tsc": "^0.40.7"
  },
  "dependencies": {
    "@vueuse/core": "^9.1.1",
    "eslint-plugin-markdown": "^3.0.0",
    "shiki": "^0.11.1"
  },
  "peerDependencyRules": {
    "ignoreMissing": [
      "vite"
    ]
  },
  "pnpm": {
    "peerDependencyRules": {
      "ignoreMissing": [
        "rollup",
        "source-map-*",
        "stable",
        "uuid",
        "vite*",
        "urix"
      ]
    }
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-customizable"
    },
    "cz-customizable": {
      "config": ".cz-config.js"
    }
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "{!(package)*.json,*.code-snippets,.!(browserslist)*rc}": [
      "prettier --write--parser json"
    ],
    "package.json": [
      "prettier --write"
    ],
    "*.vue": [
      "eslint --fix",
      "prettier --write",
      "stylelint --fix"
    ],
    "*.{scss,less,styl,html}": [
      "stylelint --fix",
      "prettier --write"
    ],
    "*.md": [
      "prettier --write"
    ]
  }
}
